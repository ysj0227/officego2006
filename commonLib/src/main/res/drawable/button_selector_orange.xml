<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true" android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dp_1" android:color="@color/common_blue_main_60" />

            <solid android:color="@color/c_white" />

            <corners android:radius="@dimen/dp_27" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dp_1" android:color="@color/common_divider" />

            <solid android:color="@color/c_white" />

            <corners android:radius="@dimen/dp_27" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dp_1" android:color="@color/common_blue_main_60" />

            <solid android:color="@color/common_divider" />

            <corners android:radius="@dimen/dp_27" />
        </shape>
    </item>

</selector>